{
    "name": "quick copy",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["./config.js", "./content.js"]
        }
    ],
    "permissions":[
        "clipboardWrite",
        "notifications"
    ]

}